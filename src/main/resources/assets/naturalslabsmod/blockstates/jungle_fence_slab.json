{
    "multipart": [
        {   "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_post" }},
		
        {   "when": { "north": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side", "y": 270, "uvlock": true }
        },
		
		{   "when": { "north_up": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_up", "uvlock": true }
        },
        {   "when": { "east_up": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_up", "y": 90, "uvlock": true }
        },
        {   "when": { "south_up": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_up", "y": 180, "uvlock": true }
        },
        {   "when": { "west_up": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_up", "y": 270, "uvlock": true }
        },
		
		{   "when": { "north_down": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_down", "uvlock": true }
        },
        {   "when": { "east_down": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_down", "y": 90, "uvlock": true }
        },
        {   "when": { "south_down": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_down", "y": 180, "uvlock": true }
        },
        {   "when": { "west_down": "true" },
            "apply": { "model": "naturalslabsmod:block/jungle_fence_slab_side_down", "y": 270, "uvlock": true }
        }
    ]
}
